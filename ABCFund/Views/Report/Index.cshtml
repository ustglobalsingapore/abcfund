@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css"></script>

    <!-- script for Performance Chart -->
    <script>
        $(document).ready(setup);

        function setup() {
            // Performance Chart
            var option = {
                type: "GET",
                url: "/Report/GetChartJSON2",
                contentType: "application/json;charset=utf-8",
                dataset: "json",
                success: performanceDataArrived
            };
            $.ajax(option);
        }

        function performanceDataArrived(data) {
            console.log(data);
            var prestigeDateLabels = data[0];
            var prestigeData = data[1];
            var stiData = data[2];
            var globalDateLabels = data[3];
            var globalData = data[4];
            var djiData = data[5];
            var nasdaqData = data[6];

            var ctx = document.getElementById('chartPrestige');
            var performanceChart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: prestigeDateLabels,
                    datasets:
                        [
                            {
                                label: 'Prestige (ABCP)',
                                backgroundColor: 'rgba(63, 191, 191, 0.8)',
                                borderColor: "rgba(63, 191, 191, 0.8)",
                                pointBorderWidth: 1,
                                pointRadius: 0,
                                pointHitRadius: 8,
                                fill: false,
                                data: prestigeData
                            },
                            {
                                label: 'STI Index (^STI)',
                                backgroundColor: 'rgba(63, 63, 191, 0.6)',
                                borderColor: "rgba(63, 63, 191, 0.6)",
                                pointBorderWidth: 1,
                                pointRadius: 0,
                                pointHitRadius: 8,
                                fill: false,
                                data: stiData
                            }
                        ]
                },
                options: {
                    responsive: true,
                    legend: {
                        position: 'bottom',
                    },
                    tooltips: {
                        mode: 'index',
                        intersect: false,
                    },
                    hover: {
                        mode: 'nearest',
                        intersect: true
                    },
                    scales: {
                        xAxes: [{
                            display: true,
                            scaleLabel: {
                                display: true,
                                labelString: 'Date'
                            },
                            ticks: {
