@model ABCFund.Models.ReportViewModel
@{
    ViewData["Title"] = "Report";
}

<body id="page-top">

    <!-- ********************** Prestige Fund Report page ********************** -->
    <!-- *********************************************************************** -->
    <!-- *********************************************************************** -->
    <!-- ABC Fund Report Title -->
    <div class="text-center">
        <h2>ABC Prestige Fund Daily Report</h2>
    </div>
    <br />

    <!-- Investment Objective -->
    <h1 class="h3 mb-2 text-gray-800">Prestige Fund Objective</h1>

    <div class="row">
        <div class="col-lg-12">
            <div class="card shadow mb-4">
                <div class="card-body">
                    <p>
                        ABC Asset Management (ABC) is one of Singapore’s oldest investment companies, created in 1968. ABC manages $20 Billion of assets, making it the largest active manager in Singapore.
                    </p>
                    <p>
                        ABC manages 2 funds, <b>Prestige Fund (ticker symbol - ABCP)</b> that caters to local Singapore market and <b>Global Fund (ticker symbol - ABCG)</b> that caters to the US market. The Prestige Fund aims at long-term capital growth by investing in local Sinagpore equity markets.
                    </p>
                </div>
            </div>
        </div>
    </div>

    <!-- Prestige Overview Chart -->
    <h1 class="h3 mb-2 text-gray-800">Prestige Overview</h1>
    <p>as of @Model.PrestigePeriodic.TradeDate.ToString("d")</p>

    <div class="row">
        <div class="col-lg-12">
            <div class="card shadow">
                <div class="card-body">
                    <canvas id="chartPrestige" class="my-4 w-100 chartjs-render-monitor" width="800" height="380" style="display: block; width: 90%; height: 40%;"></canvas>
                </div>
            </div>
        </div>
    </div>
    <br />

    <!-- Prestige Performance History -->
    <h1 class="h3 mb-2 text-gray-800">Prestige Performance History</h1>
    <p>as of @Model.PrestigePeriodic.TradeDate.ToString("d")</p>

    <div class="row">
        <div class="col-lg-12">
            <div class="card border-left-primary shadow h-100 py-2">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">
                        Prestige against Straits Time Index
                    </h6>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>
                                        Fund
                                    </th>
                                    <th>Year to Date</th>
                                    <th>1 Month</th>
                                    <th>3 Months</th>
                                    <th>6 Months</th>
                                    <th>1 Year</th>
                                    <th>Since Inception (p.a)</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        Prestige (ABCP)
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.YearToDate%
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.OneMonth%
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.ThreeMonth%
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.SixMonth%
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.OneYear%
                                    </td>
                                    <td>
                                        @Model.PrestigePeriodic.SinceInception%
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Straits Time Index (^STI)
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.YearToDate%
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.OneMonth%
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.ThreeMonth%
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.SixMonth%
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.OneYear%
                                    </td>
                                    <td>
                                        @Model.StiPeriodic.SinceInception%
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />

    <!-- Prestige Top 10 Holdings -->
    <h1 class="h3 mb-2 text-gray-800 pagebreak">Top 10 Prestige Holdings</h1>
    <p>as of @Model.PrestigePeriodic.TradeDate.ToString("d")</p>

    <div class="row">
        <div class="col-lg-12">
            <!-- DataTales Example -->
            <div class="card shadow mb-4">
