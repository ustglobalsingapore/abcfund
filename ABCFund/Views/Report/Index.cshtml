                                <tr>
                                    <th>No.</th>
                                    <th>@Html.DisplayNameFor(model => model.PrestigeHoldings[0].Weight)</th>
                                    <th>@Html.DisplayNameFor(model => model.PrestigeHoldings[0].Name)</th>
                                    <th>@Html.DisplayNameFor(model => model.PrestigeHoldings[0].Symbol)</th>
                                    <th>@Html.DisplayNameFor(model => model.PrestigeHoldings[0].Sector)</th>
                                    <th>@Html.DisplayNameFor(model => model.PrestigeHoldings[0].Industry)</th>
                                </tr>
                            </thead>
